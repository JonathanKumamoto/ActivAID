<dictionary lang="catalan">
 <stemmer> 
     <step1_pre> 
	  <rule>"|</rule> 
	  <rule>(|</rule> 
	 </step1_pre>
	 
	 
    <step1_post>
      <rule>."|</rule> 
	  <rule>,"|</rule> 
	  <rule>.|</rule>
