<?xml version="1.0"?>
<dictionary lang="irish">
 <stemmer> 
     <step1_pre> 
	  <rule>"|</rule> 
	  <rule>(|</rule> 
	 </step1_pre>
	 
	 
    <manual>
	  <rule>wrote|write</rule>
	  <rule>came|come</rule> 
	  <rule>went|go</rule>
	 </manual>
	 
    <step1_post>
      <rule>."|</rule> 
	  <rule>,"|</rule> 
	  <rule>.|</rule>
	  <rule>,|</rule> 
	  <rule>"|</rule>
	  <rule>)|</rule> 
	  <rule>?|</rule> 
	  <rule>:|</rule> 
	  <rule>;|</rule> 
	  <rule>!|</rule> 
	 </step1_post>
 
    <post>
       <rule>before1|1after</rule>
    </post>
   <pre>
       <rule>before1|1after</rule>
   </pre> 
  </stemmer>
